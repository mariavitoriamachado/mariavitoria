  <div class="mb-5">
    <div class="text-3xl w-full font-bold">Skills</div>
  </div>

  <ul class="list-disc md:columns-5 columns-2 mx-6">
    <li>HTML</li>
    <li>CSS</li>
    <li>SASS</li>
    <li>Styled-components</li>
    <li>JavaScript</li>
    <li>Reactjs</li>
    <li>Teste Unitários</li>
    <li>Typescript</li>
    <li>Java</li>
    <li>SEO</li>
    <li>MongoDB</li>
    <li>SQL</li>
    <li>Node.js</li>
    <li>Vue</li>
    <li>Vuetify</li>
    <li>Wordpress</li>
    <li>Git</li>
    <li>Jira</li>
    <li>Scrum</li>
    <li>Kanban</li>
  </ul>

  <div class="mb-5">
    <div class="text-3xl w-full font-bold"></div>
  </div>
  <div class="mb-5">
    <a class="text-3xl w-full font-bold cv-link" href="/cvmariavitoria.pdf" target="_blank">Download do CV</a>
  </div>

</BaseLayout>
